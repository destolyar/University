#include <stdio.h>

int main()
{
    int t;
    int a = 3;
    int b = 2;
    int y = 4;
    int x = 0;
    
    while (x < 6)

        if (x - y < 0)
        {
            t = ((a + b) / 2) - (a * a) + ((b * b * b) / a);
        }
        else if (x - y == 0)
        {
            t = ((a + b) / 2);
        }
        else
        {
            t = 3;    
        }
        x += 1;
        printf("t = %d", t);
}
